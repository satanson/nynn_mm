#!/bin/bash
export NYNN_MM_NAMESERV_HOST="blade4"
export NYNN_MM_NAMESERV_WORKER_NUM=$((2**${5:-2}))
export NYNN_MM_NAMESERV_SOCKET_NUM_PER_WORKER=$((2**${6:-0}))

export NYNN_MM_NAMESERV_PORT=50000
export NYNN_MM_NAMESERV_PORT_RANGE_MIN=${NYNN_MM_NAMESERV_PORT}
export NYNN_MM_NAMESERV_PORT_RANGE_MAX=$((${NYNN_MM_NAMESERV_PORT_RANGE_MIN}+$((2**${1:-0}))))
export NYNN_MM_NAMESERV_HWM=$((2**${2:-11}))
export NYNN_MM_NAMESERV_BUF=$((2**${3:-16}))
export NYNN_MM_NAMESERV_AFFINITY=$((2**${4:-0}-1))

export NYNN_MM_DATASERV_HOST_LIST="blade4 blade5 blade7 blade8"
export NYNN_MM_DATASERV_WORKER_NUM=$((2**${5:-2}))
export NYNN_MM_DATASERV_SOCKET_NUM_PER_WORKER=$((2**${6:-0}))

export NYNN_MM_DATASERV_PORT=60000
export NYNN_MM_DATASERV_PORT_RANGE_MIN=${NYNN_MM_DATASERV_PORT}
export NYNN_MM_DATASERV_PORT_RANGE_MAX=$((${NYNN_MM_DATASERV_PORT_RANGE_MIN}+$((2**${1:-0}))))
export NYNN_MM_DATASERV_HWM=${NYNN_MM_NAMESERV_HWM}
export NYNN_MM_DATASERV_BUF=${NYNN_MM_NAMESERV_BUFF}
export NYNN_MM_DATASERV_AFFINITY=${NYNN_MM_NAMESERV_AFFINITY}

export NYNN_MM_DATA_DIR="/home/bsp/programer/nynn/graph"
